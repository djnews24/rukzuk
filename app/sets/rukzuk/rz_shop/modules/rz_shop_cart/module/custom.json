  "selectors": [
    {
      "title": "__i18n_selector.links",
      "selector": ".checkoutForm a, .cartOverview a"
    },
    {
      "title": "__i18n_selector.cartTable",
      "selector": ".cartOverview .cartTable",
      "items": [
        {
          "title": "__i18n_selector.cartTableHeader",
          "selector": ".cartOverview .cartTable th"
        },
        {
          "title": "__i18n_selector.cartTableRow",
          "selector": ".cartOverview .cartTable tr.itemRow",
          "enableNthTypeOf": true
        },
        {
          "title": "__i18n_selector.cartTableRowShipping",
          "selector": ".cartOverview .cartTable tr.shippingSum"
        },
        {
          "title": "__i18n_selector.cartTableRowFullSum",
          "selector": ".cartOverview .cartTable tr.fullSum"
        },
        {
          "title": "__i18n_selector.cartTableRowVatSum",
          "selector": ".cartOverview .cartTable tr.vatSum"
        }
      ]
    },
    {
      "title": "__i18n_selector.buttons",
      "selector": ".cartButtonBar button"
    },
    {
      "title": "__i18n_selector.buttonsPrimary",
      "selector": ".cartButtonBar button.primary"
    },
    {
      "title": "__i18n_selector.checkoutForm",
      "selector": ".checkoutForm",
      "items": [
        {
          "title": "__i18n_selector.checkoutFormTextFields",
          "selector": ".checkoutForm input[type=text], .checkoutForm input[type=email], .checkoutForm input[type=tel]"
        },
        {
          "title": "__i18n_selector.checkoutFormComment",
          "selector": ".checkoutForm textarea"
        },
        {
          "title": "__i18n_selector.checkoutFormSelects",
          "selector": ".checkoutForm select"
        }
      ]
    }
  ]
